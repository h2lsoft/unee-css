{
  "name": "unee-css",
  "version": "0.9.0",
  "description": "A lightweight and customizable CSS framework",
  "main": "dist/unee.css",
  "scripts": {
    "scss-css-normal": "sass scss/unee.scss dist/unee.css",
    "post-css-normal": "postcss dist/unee.css -o dist/unee.css",
    "copy-doc": "postcss dist/unee.css -o docs/assets/css/unee.css",
    "css-compress": "sass --style=compressed dist/unee.css dist/unee.min.css",
    "build": "npm run scss-css-normal && npm run post-css-normal && npm run copy-doc && npm run css-compress"
  },
  "keywords": [
    "css",
    "framework",
    "postfix",
    "scss",
    "responsive",
    "bootstrap",
    "tailwind"
  ],
  "author": "H2lsoft <contact@h2lsoft.com>",
  "license": "MIT",
  "homepage": "https://github.com/h2lsoft/unee-css",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/h2lsoft/unee-css.git"
  },
  "bugs": {
    "url": "https://github.com/h2lsoft/unee-css/issues"
  },
  "directories": {
    "doc": "docs"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.0.6",
    "postcss": "^8.4.47",
    "postcss-cli": "^11.0.0",
    "postcss-sort-media-queries": "^5.2.0"
  }
}
